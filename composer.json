{
	"name": "harmim/images",
	"description": "A tool for working with images.",
	"keywords": ["nette", "images", "php", "gd"],
	"type": "library",
	"homepage": "https://github.com/harmim/images",
	"license": "MIT",
	"authors": [
		{
			"name": "Dominik Harmim",
			"email": "harmim6@gmail.com",
			"homepage": "http://www.harmim.cz/",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">= 7.1",
		"ext-gd": "*",
		"ext-fileinfo": "*",
		"nette/di": "^2.4",
		"nette/utils": "^2.4",
		"latte/latte": "^2.4",
		"nette/http": "^2.4",
		"nette/finder": "^2.4"
	},
	"require-dev": {
		"nette/tester": "^2.0",
		"nette/coding-standard": "^0.5"
	},
	"autoload": {
		"psr-4": {
			"Harmim\\Images\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Harmim\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"cs": "vendor/bin/ecs check src tests --config vendor/nette/coding-standard/coding-standard-php71.neon",
		"fs": "vendor/bin/ecs check src tests --config vendor/nette/coding-standard/coding-standard-php71.neon --fix"
	}
}
