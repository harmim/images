{
	"name": "harmim/images",
	"description": "Image service for Nette Framework.",
	"keywords": ["nette", "image"],
	"type": "library",
	"homepage": "https://github.com/harmim/images",
	"license": "MIT",
	"authors": [
		{
			"name": "Dominik Harmim",
			"email": "harmim6@gmail.com"
		}
	],
	"require": {
		"php": ">= 7.1",
		"ext-gd": "*",
		"ext-fileinfo": "*",
		"nette/di": "^2.4",
		"nette/utils": "^2.4",
		"latte/latte": "^2.4",
		"nette/http": "^2.4",
		"nette/finder": "^2.4"
	},
	"require-dev": {
		"nette/tester": "^1.7"
	},
	"autoload": {
		"psr-4": {
			"Harmim\\Images\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Harmim\\Tests\\": "tests/"
		}
	}
}
